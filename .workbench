{
  "fcRouteDefault": [
    {
      "apiUri": "/*",
      "fcHandler": "serverless.handler",
      "httpMethod": "ANY"
    }
  ],
  "cicd": {
    "package": "mkdir zip && rsync -a --exclude node_modules/ --exclude package-lock.json --exclude zip/ . ./zip && cd zip && npm install --production && zip -r ../code.zip ./ -x '*.git*' -x '*.zip' -x '.DS_Store' && cd ../ && rm -rf zip"
  }
}